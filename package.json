{
  "name": "nextjs-template",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "test": "jest --watch",
    "test:e2e": "cd e2e && npx codeceptjs --config codecept.local.conf.js run --steps",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test:e2e:ci": "cd e2e && npx codeceptjs run --config codecept.ci.conf.js --reporter mocha-junit-reporter",
    "serve:lhci": "NODE_ENV=production npm run start"
  },
  "dependencies": {
    "next": "^12.1.4",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/node": "^17.0.10",
    "@types/react": "17.0.38",
    "autoprefixer": "^10.4.2",
    "codeceptjs": "^3.2.3",
    "eslint": "^8.12.0",
    "eslint-config-next": "^12.1.4",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "jest-junit": "^13.0.0",
    "lint-staged": "^12.2.2",
    "mocha-junit-reporter": "^2.0.2",
    "playwright": "^1.18.0",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.6.3"
  },
  "jest-junit": {
    "outputDirectory": "reports",
    "outputName": "jest-junit.xml",
    "ancestorSeparator": " â€º ",
    "uniqueOutputName": "false",
    "suiteNameTemplate": "{filepath}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}"
  }
}
